{
  "name": "virtualized-data-table",
  "version": "0.9.10-2",
  "description": "A re-implementation of fixed-data-table using react-virtualized",
  "main": "dist/index.js",
  "scripts": {
    "compile": "rimraf dist/* && babel src -d dist --ignore test.js",
    "build:demo": "npm run clean:demo && cross-env NODE_ENV=production webpack --config webpack.config.demo.js",
    "clean:demo": "rimraf build",
    "test": "jest && ./node_modules/.bin/eslint ./src/**/*.js* --quiet --ignore-pattern *.test.js"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "table",
    "fixed-data-table",
    "react-virtualized"
  ],
  "author": {
    "name": "Paul Broman",
    "company": "PureCars / Raycom Media"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulbrom/virtualized-data-table/issues"
  },
  "homepage": "https://github.com/paulbrom/virtualized-data-table#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0",
    "cross-env": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-delay": "^0.1.0",
    "react-draggable": "^3.0.5",
    "react-virtualized": "^9.18.5"
  },
  "peerDependencies": {
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  }
}
