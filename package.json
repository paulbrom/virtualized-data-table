{
  "name": "virtualized-data-table",
  "version": "0.9.9-39",
  "description": "A re-implementation of fixed-data-table using react-virtualized",
  "main": "dist/index.js",
  "scripts": {
    "compile": "rimraf dist/* && npm run test && babel src -d dist --ignore test.js",
    "build:demo": "npm run clean:demo && cross-env NODE_ENV=production webpack --config webpack.config.demo.js",
    "clean:demo": "rimraf build",
    "test": "jest && ./node_modules/.bin/eslint ./src/**/*.js* --quiet --ignore-pattern *.test.js"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "table",
    "fixed-data-table",
    "react-virtualized"
  ],
  "author": {
    "name": "Paul Broman",
    "company": "PureCars / Raycom Media"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulbrom/virtualized-data-table/issues"
  },
  "homepage": "https://github.com/paulbrom/virtualized-data-table#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.2.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.3.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react-addons-shallow-compare": "^15.4.2",
    "react-delay": "^0.1.0",
    "react-draggable": "^3.0.5",
    "react-virtualized": "^9.18.5"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  }
}
